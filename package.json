    "react-scripts": "^4.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "deck.gl": "^8.3.11",
    "plotly.js": "^1.58.2",
    "react-map-gl": "^5.2.11",
    "react-plotly.js": "^2.5.1",
    "react-reflex": "^3.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
